#include<stdio.h>
#include<stdlib.h>
/**
* main - prints the alphabets in lowercase from a to z
* description : program prints alphabet in lower cases in aloop from a to z
* return : 0 (success)
*/
int main(void)
{
char letter;
for (letter = 'a' ; letter <= 'Z'; letter++)
{
putchar(letter);
}
putchar('\n');
return (0);
}
